<html>
    <head>
        <link rel="stylesheet" type="text/css" href="/css/app.css">

    </head>
    <body>
        <%= entries.length %>
        <% for(let entry of entries) { %>
            <div>
                <h3><%= entry.title %></h3>
                <p><%= entry.content %></p>
            </div>
        <% } %>
        

        <script type="text/javascript" src="/js/index.js"></script>
    </body>
    
</html>